<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="financeandaccounting.YearlyBudgetChartSceneController">
   <children>
      <Label layoutX="104.0" layoutY="65.0" text="Select Year:" />
      <Button layoutX="627.0" layoutY="61.0" mnemonicParsing="false" onMouseClicked="#loadButtonClick" text="Load" />
      <LineChart fx:id="lineChart" layoutX="200.0" layoutY="134.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <ComboBox fx:id="yearComboBox" layoutX="207.0" layoutY="61.0" prefWidth="150.0" />
   </children>
</AnchorPane>
